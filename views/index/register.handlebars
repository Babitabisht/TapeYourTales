<div class="container">
  <div class="row valign-wrapper">
    <div class="col s9 offset-s3 valign">
      <div class="card">
        <div class="card-content white-text">
          <span class="card-title">
            <h3 style="color:black">Register </h3>
          </span>


          <form action="/register" method="POST">

            <div class="row">
              <div class="input-field col s12">
                <input name="email" type="email" class="validate">
                <label for="email">Email</label>
              </div>
            </div>

            <div class="row">
              <div class="input-field col s12">
                <input name="first_name" type="text" class="validate">
                <label for="first_name">First Name</label>
              </div>
            </div>

            <div class="row">
              <div class="input-field col s12">
                <input name="last_name" type="text" class="validate">
                <label for="last_name">Last Name</label>
              </div>

            </div>

            <div class="row">
              <div class="input-field col s12">
                <input name="password" type="password" class="validate">
                <label for="password">Password</label>
              </div>
            </div>

            <div class="row">
              <div class="input-field col s12">
                <input name="Cpassword" type="password" class="validate">
                <label for="Confirm_password">Confirm Password</label>
              </div>
            </div>
            <input type="submit" value="Submit" class="btn-large  #2196f3 blue waves-effect waves-light">
          </form>


        </div>


        
      </div>
    </div>
  </div>
</div>